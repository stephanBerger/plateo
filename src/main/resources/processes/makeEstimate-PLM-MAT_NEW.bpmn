  <process id="makeEstimate-PLM-MAT_NEW" name="Prestation de plomberie" isExecutable="true">
    <startEvent id="startevent1" name="SuperStart"></startEvent>
    <userTask id="form-1" name="Conditions d'installation" default="flow7" activiti:assignee="${account.id}">
      <extensionElements>
        <activiti:formProperty id="deposeMaterielExistant" name="Dépose du matériel existant" type="boolean" variable="deposeMaterielExistant" default="false"></activiti:formProperty>
        <activiti:formProperty id="evacuationMaterielExistant" name="Évacuation du matériel existant" type="boolean" variable="evacuationMaterielExistant" default="false"></activiti:formProperty>
        <activiti:formProperty id="modificationEmplacement" name="Modification de l'emplacement ou pose du nouvel appareil au même endroit" type="boolean" variable="modificationEmplacement" default="false"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow3" sourceRef="startevent1" targetRef="form-1"></sequenceFlow>
    <userTask id="usertask1" name="Modification de l'emplacement" activiti:assignee="${account.id}">
      <extensionElements>
        <activiti:formProperty id="cheminementReseaux" name="Cheminement des réseaux" type="enum">
          <activiti:value id="apparent" name="En apparent"></activiti:value>
          <activiti:value id="videSanitaire" name="En vide sanitaire"></activiti:value>
          <activiti:value id="saigneeMur" name="Saignée dans mur béton ou briques"></activiti:value>
          <activiti:value id="saigneeCloison" name="Saignée dans cloison"></activiti:value>
        </activiti:formProperty>
        <activiti:formProperty id="distanceNouvelEmplacement" name="Distance entre ancien et nouvel emplacement (m)" type="long"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow7" sourceRef="form-1" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow8" name="Si modification emplacement" sourceRef="exclusivegateway1" targetRef="usertask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${modificationEmplacement}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask2" name="Choix du matériel" activiti:assignee="${account.id}">
      <extensionElements>
        <activiti:formProperty id="choixMateriel" name="Liste des matériels pris en charge" type="enum">
          <activiti:value id="chauffeEau" name="Chauffe eau"></activiti:value>
          <activiti:value id="douche" name="Douche"></activiti:value>
          <activiti:value id="baignoire" name="Baignoire"></activiti:value>
          <activiti:value id="lavabo" name="Lavabo"></activiti:value>
        </activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow9" sourceRef="exclusivegateway1" targetRef="usertask2"></sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="usertask1" targetRef="usertask2"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow11" sourceRef="usertask2" targetRef="endevent1"></sequenceFlow>
  </process>
